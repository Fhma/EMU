//13) Binding Feature Change: Binding_propertyName_replace
@action replace
@feature propertyName
pattern mutation_operator_13_case_1
instance:Binding
{
	do {
		instance.propertyName = instance.propertyName.firstToUpperCase();
	}
}

@action replace
@feature propertyName
@type Binding
pattern mutation_operator_13_case_2
instance1:Binding,
otherbindings:Binding from: Binding.all.select(e|e <> instance1)
{
	do {
		instance1.propertyName = otherbindings.propertyName;
	}
}
